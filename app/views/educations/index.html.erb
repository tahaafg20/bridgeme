<p id="notice"><%= notice %></p>

<h1>Educations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th>Email</th>
      <th>About</th>
      <th>Accepted documents</th>
      <th>Services</th>
      <th>Status</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educations.each do |education| %>
      <tr>
        <td><%= education.name %></td>
        <td><%= education.number %></td>
        <td><%= education.email %></td>
        <td><%= education.about %></td>
        <td><%= education.accepted_documents %></td>
        <td><%= education.services %></td>
        <td><%= education.status %></td>
        <td><%= education.latitude %></td>
        <td><%= education.longitude %></td>
        <td><%= link_to 'Show', education %></td>
        <td><%= link_to 'Edit', edit_education_path(education) %></td>
        <td><%= link_to 'Destroy', education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education', new_education_path %>
