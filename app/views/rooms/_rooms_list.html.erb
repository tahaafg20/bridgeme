<% rooms.each do |room| %>
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading preview">
          <%= image_tag(url_for(room.cover),size:"300x300") %>
        </div>
        <div class="panel-body">
          <%= link_to room.listing_name, room %><br/>
          RM<%= room.price %> - <%= room.home_type %> - <%= pluralize(room.bed_room, "bed") %>
        </div>
      </div>
    </div>
<% end %>
