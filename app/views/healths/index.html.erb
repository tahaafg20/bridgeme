<p id="notice"><%= notice %></p>

<h1>Healths</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th>Link</th>
      <th>Email</th>
      <th>Services</th>
      <th>About</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @healths.each do |health| %>
      <tr>
        <td><%= health.name %></td>
        <td><%= health.number %></td>
        <td><%= health.link %></td>
        <td><%= health.email %></td>
        <td><%= health.services %></td>
        <td><%= health.about %></td>
        <td><%= health.address %></td>
        <td><%= health.latitude %></td>
        <td><%= health.longitude %></td>
        <td><%= link_to 'Show', health %></td>
        <td><%= link_to 'Edit', edit_health_path(health) %></td>
        <td><%= link_to 'Destroy', health, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health', new_health_path %>
